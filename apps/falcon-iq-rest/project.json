{
  "name": "falcon-iq-rest",
  "projectType": "application",
  "root": "apps/falcon-iq-rest",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/falcon-iq-rest",
        "command": "mvn package"
      },
      "outputs": ["apps/falcon-iq-rest/target"]
    },
    "build:dev": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/falcon-iq-rest",
        "command": "mvn -DskipTests compile"
      }
    },
    "build:standalone": {
      "executor": "nx:run-commands",
      "dependsOn": ["clean"],
      "options": {
        "cwd": "apps/falcon-iq-rest",
        "command": "mvn package -Pstandalone -DskipTests"
      }
    },
    "clean": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/falcon-iq-rest",
        "command": "mvn clean"
      }
    },
    "dev": {
      "executor": "nx:run-commands",
      "dependsOn": ["build:dev"],
      "options": {
        "cwd": "apps/falcon-iq-rest",
        "command": "mvn -DskipTests jetty:run"
      }
    }
  },
  "tags": []
}
