{
  "name": "falcon-iq-rest",
  "projectType": "application",
  "root": "apps/falcon-iq-rest",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "mvn package"
      },
      "outputs": ["apps/falcon-iq-rest/target"]
    },
    "run:dev": {
      "executor": "nx:run-commands",
      "dependsOn": ["build:standalone"],
      "options": {
        "cwd": "apps/falcon-iq-rest",
        "command": "java -jar target/falcon-iq-rest-1.0.0-standalone.jar"
      }
    },
    "build:standalone": {
      "executor": "nx:run-commands",
      "dependsOn": ["clean"],
      "options": {
        "cwd": "apps/falcon-iq-rest",
        "command": "mvn package -Pstandalone -DskipTests"
      }
    },
    "clean": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/falcon-iq-rest",
        "command": "mvn clean"
      }
    }
  },
  "tags": []
}
