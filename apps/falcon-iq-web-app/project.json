{
  "name": "falcon-iq-web-app",
  "projectType": "application",
  "root": "apps/falcon-iq-web-app",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/falcon-iq-web-app",
        "command": "next build"
      }
    },
    "build:cloudflare": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/falcon-iq-web-app",
        "command": "opennextjs-cloudflare build"
      }
    },
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/falcon-iq-web-app",
        "commands": ["nx run falcon-iq-rest:dev", "next dev"]
      }
    },
    "dev:cloudflare": {
      "dependsOn": ["build:cloudflare"],
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/falcon-iq-web-app",
        "command": "opennextjs-cloudflare preview"
      }
    }
  },
  "tags": []
}
